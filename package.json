{
  "name": "changesets-changelog-github-local",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.15.1",
  "description": "A local-only version of '@changesets/changelog-github' that doesn't rely on GitHub's API.",
  "author": "LekoArts",
  "license": "MIT",
  "homepage": "https://github.com/author/library#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/author/library.git"
  },
  "bugs": {
    "url": "https://github.com/author/library/issues"
  },
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && npm publish",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.2",
    "@changesets/parse": "^0.4.1",
    "@changesets/types": "^6.1.0",
    "@types/node": "^22.15.17",
    "bumpp": "^10.1.0",
    "eslint": "^9.34.0",
    "eslint-plugin-format": "^1.0.1",
    "tsdown": "^0.11.9",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  }
}
